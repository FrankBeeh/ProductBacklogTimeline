<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane maxHeight="-1.0" maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="768.0" prefWidth="1024.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="de.frankbeeh.productbacklogtimeline.view.ProductBacklogTimelineMainController">
	<top>
		<MenuBar>
			<menus>
				<Menu mnemonicParsing="false" text="File">
					<items>
						<MenuItem mnemonicParsing="false" onAction="#importProductBacklog" text="Import PBL" />
						<MenuItem mnemonicParsing="false" onAction="#importSprints" text="Import Sprints" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
	</top>
	<center>
		<TabPane prefHeight="-1.0" prefWidth="-1.0" tabClosingPolicy="UNAVAILABLE">
			<tabs>
				<Tab text="PBL">
					<content>
						<AnchorPane  minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
							<children>
								<TableView fx:id="productBacklogTable" prefHeight="743.0" prefWidth="1023.0" BorderPane.alignment="CENTER">
									<columnResizePolicy>
										<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
									</columnResizePolicy>
									<columns>
										<TableColumn editable="false" maxWidth="70.0" minWidth="20.0" prefWidth="-1.0" resizable="true" sortable="false" text="Id" fx:id="idColumn" />
										<TableColumn editable="false" maxWidth="60.0" minWidth="60.0" prefWidth="-1.0" resizable="true" sortable="false" text="Estimate" fx:id="estimateColumn" />
										<TableColumn editable="false" maxWidth="5000.0" minWidth="200.0" prefWidth="-1.0" resizable="true" sortable="false" text="Title" fx:id="titleColumn" />
										<TableColumn editable="false" maxWidth="5000.0" minWidth="200.0" prefWidth="-1.0" resizable="true" sortable="false" text="Description" fx:id="descriptionColumn" />
									</columns>
								</TableView>
							</children>
						</AnchorPane>
					</content>
				</Tab>
				<Tab text="Sprints">
					<content>
						<AnchorPane  minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
							<children>
								<TableView fx:id="sprintsTable" prefHeight="743.0" prefWidth="1023.0" BorderPane.alignment="CENTER">
									<columnResizePolicy>
										<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
									</columnResizePolicy>
									<columns>
										<TableColumn editable="false" maxWidth="200.0" minWidth="40.0" prefWidth="100.0" resizable="true" sortable="false" text="Name" fx:id="nameColumn" />
										<TableColumn editable="false" maxWidth="100.0" minWidth="40.0" prefWidth="100.0" resizable="true" sortable="false" text="Start Date" fx:id="startDateColumn" />
										<TableColumn editable="false" maxWidth="100.0" minWidth="40.0" prefWidth="100.0" resizable="true" sortable="false" text="End Date" fx:id="endDateColumn" />
										<TableColumn editable="false" maxWidth="100.0" minWidth="20.0" prefWidth="100.0" resizable="true" sortable="false" text="Planned Capacity" fx:id="plannedCapacityColumn" />
										<TableColumn editable="false" maxWidth="100.0" minWidth="20.0" prefWidth="100.0" resizable="true" sortable="false" text="Planned Effort" fx:id="plannedEffortColumn" />
										<TableColumn editable="false" maxWidth="100.0" minWidth="20.0" prefWidth="100.0" resizable="true" sortable="false" text="Actual Capacity" fx:id="actualCapacityColumn" />
										<TableColumn editable="false" maxWidth="100.0" minWidth="20.0" prefWidth="100.0" resizable="true" sortable="false" text="Effort Done" fx:id="effortDoneColumn" />
									</columns>
								</TableView>
							</children>
						</AnchorPane>
					</content>
				</Tab>
			</tabs>
		</TabPane>
	</center>
</BorderPane>
