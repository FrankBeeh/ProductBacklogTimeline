<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import de.frankbeeh.productbacklogtimeline.view.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?scenebuilder-classpath-element ../../../../../../bin?>
<?import javafx.scene.control.ContextMenu?>
<?import javafx.scene.control.MenuItem?>

<TableView fx:id="velocityForecastTable" prefHeight="719" prefWidth="1198" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="de.frankbeeh.productbacklogtimeline.view.VelocityForecastTableController">
  <columnResizePolicy>
    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
  </columnResizePolicy>
  <columns>
    <TableColumn editable="false" maxWidth="100.0" minWidth="40.0" prefWidth="100.0" resizable="true" sortable="false" text="Name" fx:id="nameColumn">
      <cellValueFactory>
        <PropertyValueFactory property="name"/>
      </cellValueFactory>
    </TableColumn>
    <TableColumn editable="false" maxWidth="65.0" minWidth="40.0" prefWidth="65.0" resizable="true" sortable="false" text="Start Date" fx:id="startDateColumn" />
    <TableColumn editable="false" maxWidth="65.0" minWidth="40.0" prefWidth="65.0" resizable="true" sortable="false" text="End Date" fx:id="endDateColumn" />
    <TableColumn prefWidth="75.0" text="Per Sprint">
      <columns>
        <TableColumn prefWidth="75.0" text="Planned">
          <columns>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Capacity" fx:id="capacityForecastColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		  <cellValueFactory>
      		    <PropertyValueFactory property="capacityForecast"/>
      		  </cellValueFactory>
      		</TableColumn>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Effort" fx:id="effortForecastColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		  <cellValueFactory>
      		    <PropertyValueFactory property="effortForecast"/>
      		  </cellValueFactory>
      		</TableColumn>
          </columns>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Done">
          <columns>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Capacity" fx:id="capacityDoneColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		  <cellValueFactory>
      		    <PropertyValueFactory property="capacityDone"/>
      		  </cellValueFactory>
      		</TableColumn>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Effort" fx:id="effortDoneColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		  <cellValueFactory>
      		    <PropertyValueFactory property="effortDone"/>
      		  </cellValueFactory>
      		</TableColumn>
          </columns>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Progress Forecast">
          <columns>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Min. Vel." fx:id="forecastPerSprintByMinVelColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		</TableColumn>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Avg. Vel." fx:id="forecastPerSprintByAvgVelColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		</TableColumn>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Max. Vel." fx:id="forecastPerSprintByMaxVelColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		</TableColumn>
          </columns>
        </TableColumn>
      </columns>
    </TableColumn>
    <TableColumn prefWidth="75.0" text="Accumulated">
      <columns>
        <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Done" fx:id="accumulatedEffortDoneColumn" >
          <cellFactory>
            <AlignRightCellFactory />
      	  </cellFactory>
      	  <cellValueFactory>
      	    <PropertyValueFactory property="accumulatedEffortDone"/>
      	  </cellValueFactory>
        </TableColumn>
        <TableColumn prefWidth="75.0" text="Progress Forecast">
          <columns>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Min. Vel." fx:id="accumulatedForecastByMinVelColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		</TableColumn>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Avg. Vel." fx:id="accumulatedForecastByAvgVelColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		</TableColumn>
            <TableColumn editable="false" maxWidth="60.0" minWidth="20.0" prefWidth="60.0" resizable="true" sortable="false" text="Max. Vel." fx:id="accumulatedForecastByMaxVelColumn" >
              <cellFactory>
        		<AlignRightCellFactory />
      		  </cellFactory>
      		</TableColumn>
          </columns>
        </TableColumn>
      </columns>
    </TableColumn>
  </columns>
  <contextMenu>
  	<ContextMenu>
  		<items>
  			<MenuItem text="edit Sprint" fx:id="editItem" onAction="#editItem" />
  		</items>
  	</ContextMenu>
  </contextMenu>
</TableView>
